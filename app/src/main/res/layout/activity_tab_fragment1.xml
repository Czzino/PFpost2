<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#F4F0F0"
    android:padding="10dp">


    <RelativeLayout
        android:id="@+id/rela1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFFFF"
        android:padding="5dp"
        android:background="@drawable/shape_line">

        <TextView
            android:id="@+id/tv_company"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="CJ대한통운"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="20sp"/>


        <TextView
            android:id="@+id/tv_goods"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="삼성노트북17인치"
            android:gravity="start"
            android:paddingLeft="12dp"
            android:maxLines="2"
            android:layout_alignBottom="@+id/tv_booking"
            android:layout_toRightOf="@+id/tv_company"
            android:textStyle="bold"
            android:textSize="18sp"/>


        <TextView
            android:id="@+id/tv_from"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/tv_num"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:paddingLeft="12dp"
            android:textStyle="bold"
            android:text="From : 블*"
            android:textSize="17sp"
            android:layout_alignBottom="@+id/tv_num"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7.23 목요일"
            android:textStyle="bold"
            android:textSize="14sp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="14dp"
            android:layout_alignBottom="@+id/tv_num"/>




        <TextView
            android:id="@+id/tv_booking"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="택배예약"
            android:layout_marginTop="3dp"
            android:textStyle="bold"
            android:textColor="#4FAC2A"
            android:textSize="20sp"
            android:layout_below="@id/tv_num"
            android:gravity="center"/>

        <TextView
            android:id="@+id/tv_num"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:gravity="center"
            android:layout_below="@+id/tv_company"
            android:text="344056120884"/>



    </RelativeLayout>

    <RelativeLayout
